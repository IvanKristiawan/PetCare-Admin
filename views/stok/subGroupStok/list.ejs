<style>
    a {
        color: inherit; /* blue colors for links too */
        text-decoration: inherit; /* no underline */
    }
</style>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">Kode</th>
            <th scope="col">Nama SubGroup</th>
            <th scope="col">Group</th>
        </tr>
    </thead>
    <tbody>
        <% for(let subGroupStok of subGroupStoks) {%>
            <tr>
                <th scope="row"><%= subGroupStok.kodeSubGroup %> </th>
                <td><a href="/stoks/subGroupStok/<%= subGroupStok._id %>"><%= subGroupStok.namaSubGroup %></a></td>
                <td><%= subGroupStok.kode %> - <%= subGroupStok.namaGroupStok %></td>
            </tr>
        <% } %>     
    </tbody>
</table>